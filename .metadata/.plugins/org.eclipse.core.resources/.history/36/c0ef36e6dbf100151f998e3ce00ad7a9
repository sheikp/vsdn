				$(document).ready(function(){
				var he = $(window).height();        //Get the height of the browser window
				var wi = $(window).width();
				$('#topo').height(he ); 				//Resize the pageContent div, with a size of 60 - page height.				
				$('#topo').width((wi - 230));
				$('#ftopo').height(he ); 				
				$('#ftopo').width((wi - 230));				
				$('#topo').css({ top: "0px", left: "230px" })		
				
				});
